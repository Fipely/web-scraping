# Configurações da API FIPE
FIPE_BASE_URL=https://veiculos.fipe.org.br/api/veiculos/

# Headers para requisições
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36
REFERER=https://veiculos.fipe.org.br/

# Configurações de retry e rate limiting
MAX_RETRIES=5
INITIAL_BACKOFF=2.0
MAX_BACKOFF=120.0
BACKOFF_MULTIPLIER=2.0
REQUEST_TIMEOUT=30
DELAY_BETWEEN_REQUESTS=1.5

# Configurações de processamento paralelo
MAX_WORKERS=4

# Diretório de saída
OUTPUT_DIR=output
PARTIAL_OUTPUT_DIR=output/partial
FINAL_OUTPUT_FILE=output/fipe_complete.json

# Logging
LOG_LEVEL=INFO
LOG_FILE=output/fipe_scraper.log
